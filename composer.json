{
  "name": "slakbal/gotowebinar",
  "description": "GotoWebinar API wrapper for laravel 5+",
  "license": "MIT",
  "keywords": [
    "gotowebinar",
    "webinar",
    "laravel"
  ],
  "authors": [
    {
      "name": "Leslie Price",
      "email": "leslie.price78@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "nategood/httpful": "^0.2",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
    "guzzlehttp/guzzle": "~6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Slakbal\\Gotowebinar\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slakbal\\Gotowebinar\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Slakbal\\Gotowebinar\\GotoWebinarServiceProvider"
      ],
      "aliases": {
        "GotoWebinar": "Slakbal\\Gotowebinar\\Facade\\GotoWebinar"
      }
    }
  },
  "minimum-stability": "stable"
}
